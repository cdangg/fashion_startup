<div class="container">

  <div class="header-container">
      <div class="logo-container">
        <%= image_tag("logo.png") %>
      </div>
      <div class="headline">
        <h1 class="oxygen">STARTUP FASHION WEEK</h1>
        <h2 class="oxygen2">Discover | Connect | Accelerate</h2>
         <div class="social-icons">
            <a href="#"><%= image_tag(("twitter_icon.png"), :class => "social")%></a>
             <a href="#"><%= image_tag(("facebook_icon.png"), :class => "social")%></a>
              <a href="#"><%= image_tag(("instagram_icon.png"), :class => "social")%></a>
          </div>
        </div>
          </div>

    <div class="nav-width">
      <nav class="top-bar" data-topbar data-options="sticky_on: large">
    <ul class="title-area">
       <!-- Remove the class "menu-icon" to get rid of menu icon. Take out "Menu" to just have icon alone -->
      <li class="toggle-topbar menu-icon"><a href="#"><span>Menu</span></a></li>
    </ul>

    <section class="top-bar-section">
      <!-- Right Nav Section -->
      <ul class="right">
            <li><%= link_to "HOME", welcome_index_path %></li>
              <li><%= link_to "ABOUT", welcome_about_path %></li>
              <li><%= link_to "THE TEAM", teams_path %></li>
              <li><%= link_to "EVENTS", welcome_index_path %></li>
              <li><%= link_to "SPEAKERS", welcome_index_path %></li>
              <li><%= link_to "SPONSORS", welcome_index_path %></li>
              <li><%= link_to "MEDIA", welcome_media_path %></li>
              <li><%= link_to "CONTACT US", welcome_contact_path %></li>
      </ul>

    </section>
  </nav>
  <div class="body-content-wrapper">
            <div class="sidebar">
              <ul>
                <li><a href="#Sponsor">Sponsor Spotlight</a></li>
                <li><a href="#Description">Sponsor Description</a></li>
                <li><a href="#Affiliation">Sponsor Affiliation</a></li>
              </ul>
            </div>
            <div id="main">
                <h1><a name="Sponsor">SPONSOR SPOTLIGHT</a></h1>
                <hr />
                <div style="height: 199px; overflow: visible;"><%= image_tag @sponsor.cover_image_url.to_s, :size => "679x250" %></div>
                <%= image_tag @sponsor.logo_url.to_s, :style => "width: 150px; margin-left: 30px; border: 3px solid white" %>
                  <% if !@sponsor.twitter_link.empty? %>
                    <%= link_to @sponsor.twitter_link do %>
                      <i class="fa fa-twitter-square fa-2x"></i>
                    <% end %>
                  <% end %>
                  <% if !@sponsor.facebook_link.empty? %>
                    <%= link_to @sponsor.facebook_link do %>
                      <i class="fa fa-facebook-square fa-2x"></i>
                    <% end %>
                  <% end %>
                <p>
                    <strong><%= @sponsor.name %></strong><br />
                    <%= link_to @sponsor.url, @sponsor.url %><br />
                    <%= @sponsor.address %><br />
                    <%= @sponsor.email unless @sponsor.email.empty? %><br />
                    <%= @sponsor.phone_number unless @sponsor.phone_number.empty? %><br />
                    <a name="Description">Company Description:</a><br />
                    <%= @sponsor.description %><br><br>
                    <a name="Affiliation">Sponsor Affiliation:</a><br>
                    <%= @sponsor.affiliation %>
                </p>
            </div>
        </div>
        <div class="footer">
          <div class="ticker" id="msg_ticker">
              <ul>
                <li><a href="#">Sign up for our Newsletter</a></li>
                <li><a href="#">Message 2</a></li>
                <li><a href="#">Message 3</a></li>
                <li><a href="#">Message 4</a></li>
              </ul>
            </div>

            <a href="#"><div class="click"><p>Click Here</p></div></a>
        </div>
    </div>
</div>