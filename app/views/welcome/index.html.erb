  <%= image_tag "http://placehold.it/250x250&text=Thumbnail", :class => "image" %>
  <%= image_tag "http://placehold.it/250x250&text=Thumbnail", :class => "image" %>
  <%= image_tag "http://placehold.it/250x250&text=Thumbnail", :class => "image" %>
</div>


