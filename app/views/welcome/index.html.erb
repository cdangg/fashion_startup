<!-- for margin top and bottom: 30px -->
<div id="welcome" class="wrapper">

<!-- do NOT remove this row is use to clear the inner padding -->
<div class="row">

  <!-- 1st row -->
  <div class="row">
    <div class="medium-8 large-9 column">

      <!-- flixel -->
      <div id="flixel" class="container">
        <%= image_tag "fashion.gif", class: 'flixel' %>
      </div>

    </div>
    <div class="medium-4 large-3 column">

      <!-- feature-sponsor-content -->
      <div id="feature-sponsor" class="table">
        <div class="content table-inner">
          <p>Hello World</p>
        </div>
      </div>

    </div>
  </div>
  <!-- 2nd row -->
  <%= render 'layouts/message_ticker' %>

  <div class="row">
    <div class="thumbnails">
      <!-- thumbnails -->

      <div class="small-12 medium-6 large-3 column">
        <div class="thumbnail" style="background-image: url(<%= image_url('manequins.JPG') %>)">
          <div class="hover">
          <%= link_to @startup, class: 'caption' do %>
            <p>DISCOVER WHO IS IN OUR STARTUP FASHION SPOTLIGHT THIS WEEK</p>
          <% end %>
          </div>
        </div>
        <h6 class="panel" id="panel-label"><%= link_to "Startup Spotlight", @startup %></h6>
      </div>

      <div class="small-12 medium-6 large-3 column">
        <div class="thumbnail" style="background-image: url(<%= image_url('sponsor_spot.jpg') %>)">
          <div class="hover">
          <%= link_to @sponsor, class: 'caption' do %>
            <p>DISCOVER WHO IS IN OUR SPONSOR SPOTLIGHT THIS WEEK</p>
          <% end %>
          </div>
        </div>
        <h6 class="panel" id="panel-label"><%= link_to "Sponsor Spotlight", @sponsor %></h6>
      </div>

      <div class="small-12 medium-6 large-3 column">
        <div class="thumbnail" style="background-image: url(<%= image_url('event_spot.jpg') %>)">
          <div class="hover">
          <%= link_to @event, class: 'caption' do %>
            <p>DISCOVER WHAT IS HAPPENING NOW</p>
          <% end %>
          </div>
        </div>
        <h6 class="panel" id="panel-label"><%= link_to "Event Spotlight", @event %></h6>
      </div>

      <div class="small-12 medium-6 large-3 column">
        <div class="thumbnail" style="background-image: url(<%= image_url('contest_spot.jpg') %>)">
          <div class="hover">
            <%= link_to @contest, class: 'caption' do %>
              <p>DISCOVER WHAT IS HAPPENING NOW</p>
            <% end %>
          </div>
        </div>
        <h6 class="panel" id="panel-label"><a href="#" data-reveal-id="myModal" style="color: #000" data-reveal>Contests Spotlight</a></h6>
        <div id="myModal" class="reveal-modal" data-reveal>
          <h3>Let's get in touch!</h3>
          <p class="lead">Subscribe, subscribe!</p>
          <p><%= render 'form' %></p>
          <a class="close-reveal-modal">&#215;</a>
        </div>
      </div>

    </div>
  </div>
  <div class="row">
    <div class="small-12 column">
      <div class="headline"><h2 class="headline2">Discover | Connect | Accelerate</h2></div>
    </div>
  </div>

</div>
</div><!-- end of wrapper -->


